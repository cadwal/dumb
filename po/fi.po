#: tool/wadtool.c:60
#, c-format
msgid ""
"Usage: %s [OPTION FILE [FILE]...]...\n"
"Extract, copy and concatenate WAD files.\n"
"\n"
msgstr ""
"Käyttö: %s [VALITSIN TIEDOSTO [TIEDOSTO]...]...\n"
"Pura, kopioi ja kokoa WAD-tiedostoja.\n"
"\n"

#: tool/wadtool.c:64
msgid ""
"  -r, --read-iwad=FILE               read this file as an IWAD\n"
"  -p, --read-pwad=FILE [FILE]...     read these files as PWADs\n"
"  -w, --output-pwad=FILE             start writing output to this PWAD\n"
"  -W, --output-iwad=FILE             start writing output to this IWAD\n"
"  -D, --output-directory=DIRECTORY   start writing output to this directory\n"
"  -c, --copy-wad=FILE [FILE]...      copy all lumps in these wadfiles\n"
"                                     (without patching)\n"
"  -l, --copy-lump=LUMP [LUMP]...     copy these lumps to the current wad\n"
"  -L, --empty-lump=LUMP [LUMP]...    spit out an empty lump\n"
"  -x, --extract-lump=LUMP [LUMP]...  copy these lumps to raw files\n"
"  -f, --read-lump=FILE [FILE]...     copy raw files to lumps\n"
"                                     (guessing lump names)\n"
"  -F, --append-lump=FILE [FILE]...   copy raw files to lump\n"
"                                     (adding to current)\n"
"  -n, --rename-lump OLD NEW          copy lump OLD to lump NEW\n"
"      --help                         display this help and exit\n"
"      --version                      output version information and exit\n"
"\n"
msgstr ""
"  -r, --read-iwad=TIEDOSTO           lue tiedosto IWAD:na\n"
"  -p, --read-pwad=TIEDOSTO [TIEDOSTO]...\n"
"                                     lue nämä tiedostot PWAD:eina\n"
"  -w, --output-pwad=TIEDOSTO         ala kirjoittaa tätä PWAD:ia\n"
"  -W, --output-iwad=TIEDOSTO         ala kirjoittaa tätä IWAD:ia\n"
"  -D, --output-directory=HAKEMISTO   ala kirjoittaa tähän hakemistoon\n"
"  -c, --copy-wad=TIEDOSTO [TIEDOSTO]...\n"
"                                     kopioi kaikki köntit näistä WAD-\n"
"                                     tiedostoista (yhdistelemättä)\n"
"  -l, --copy-lump=KÖNTTI [KÖNTTI]... kopioi nämä köntit nykyiseen "
"kohteeseen\n"
"  -L, --empty-lump=KÖNTTI [KÖNTTI]...\n"
"                                     kyhää tyhjä köntti\n"
"  -x, --extract-lump=KÖNTTI [KÖNTTI]...\n"
"                                     kopioi nämä köntit tiedostoiksi\n"
"  -f, --read-lump=TIEDOSTO [TIEDOSTO]...\n"
"                                     kopioi tiedostot sopivan nimisiksi\n"
"                                     könteiksi\n"
"  -F, --append-lump=TIEDOSTO [TIEDOSTO]...\n"
"                                     kopioi tiedostot edellisen köntin "
"jatkoksi\n"
"  -n, --rename-lump VANHA UUSI       kopioi VANHA köntti UUDEN nimiseksi\n"
"      --help                         näytä tämä ohje ja lopeta\n"
"      --version                      näytä versiotiedot ja lopeta\n"
"\n"

#. " -X <s> <d>   : copy lump <s> to raw file <d>\n"
#: tool/wadtool.c:84
msgid ""
"Usage:  wadtool <option> <file> <file>... <option> <file>...\n"
"\n"
"Options:\n"
" -r <wadfile> : read this file as an IWAD\n"
" -p <wadfile> : read these files as PWADs\n"
" -w <wadfile> : start writing output to this PWAD\n"
" -W <wadfile> : start writing output to this IWAD\n"
" -D <dir>     : start writing output to this directory\n"
" -c <wadfile> : copy all lumps in this wadfile (without patching)\n"
" -l <lump>    : copy these lumps to the current wad\n"
" -L <lump>    : spit out an empty lump\n"
" -x <lump>    : copy these lumps to raw files\n"
" -f <rawfile> : copy raw file to lump (guessing lumpname)\n"
" -F <rawfile> : copy raw file to lump (adding to current)\n"
" -n <s> <d>   : copy lump 's' to lump 'd'\n"
"\n"
msgstr ""
"Käyttö: wadtool <valitsin> <tiedosto> <tiedosto>... <valitsin> "
"<tiedosto>...\n"
"\n"
"Valitsimet:\n"
" -r <wadtiedosto>    : lue tämä tiedosto IWAD:na\n"
" -p <wadtiedosto>    : lue nämä tiedostot PWAD:eina\n"
" -w <wadtiedosto>    : ala kirjoittaa tätä PWAD:ia\n"
" -W <wadtiedosto>    : ala kirjoittaa tätä IWAD:ia\n"
" -D <hakemisto>      : ala kirjoittaa tähän hakemistoon\n"
" -c <wadtiedosto>    : kopioi kaikki tämän tiedoston köntit "
"(yhdistelemättä)\n"
" -l <köntti>         : kopioi nämä köntit valittuun wadiin\n"
" -L <köntti>         : tee tyhjä köntti\n"
" -x <köntti>         : kopioi nämä köntit paljaisiin tiedostoihin\n"
" -f <paljas tiedosto>: kopioi köntiksi (sopivan nimiseksi)\n"
" -F <paljas tiedosto>: kopioi könttiin (äskeisen jatkoksi)\n"
" -n <l> <k>          : kopioi köntti \"l\" köntiksi \"k\"\n"
"\n"

