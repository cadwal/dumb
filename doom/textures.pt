// IDEA: (use the phase-engine for doing texture animations too)
//
// (alternatively, this stuff could be compiled into the ANIMTEX.lump)

#define AnimTexTable  PhaseTable
#define AnimTexRate   220msec

#define _Flat(name)     
#define _Texture(name)  

#define SigSwitchOn
#define SigSwitchOff

#define Texture  AnimType 0
#define Flat     AnimType 1
#define Switch   AnimType 2


